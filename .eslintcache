ate form\n  const goBack = () => {\n    props.history.push(`/items/${item._id}`)\n  }\n\n  return (\n    <div className=\"list-div\">\n      <h1 className=\"header\">Update {item.title}</h1>\n      <Formik\n        onSubmit={(values) => {\n          updateItem(values)\n        }}\n        validationSchema={validationSchema}\n        initialValues={{\n          title: item.title,\n          email: item.email,\n          price: item.price,\n          description: item.description,\n          imageUrl: item.imageUrl,\n          category: item.category,\n        }}\n      >\n        {({ handleSubmit }) => (\n          <form className=\"form\" onSubmit={handleSubmit}>\n            <div className=\"form-div\">\n              <MySelect name=\"category\" data={item.category} />\n              <MyInput name=\"title\" data={item.title} />\n              <MyInput name=\"price\" data={item.price} />\n              <MyInput name=\"imageUrl\" data={item.imageUrl} />\n              <MyInput name=\"email\" data={item.email} />\n              <MyInput name=\"description\" data={item.description} />\n              <div className=\"form-btns\">\n                <div className=\"btn\">\n                  <Button variant=\"outlined\" color=\"secondary\" onClick={goBack}>\n                    Go Back\n                  </Button>\n                </div>\n                <div className=\"btn\">\n                  <Button variant=\"contained\" color=\"primary\" type=\"submit\">\n                    Update\n                  </Button>\n                </div>\n              </div>\n            </div>\n          </form>\n        )}\n      </Formik>\n    </div>\n  )\n}\n\nexport default UpdateItem\n","/Users/evandoherty/Desktop/Coding/Specialisterne/w3-project/client/src/models/item.js",[],"/Users/evandoherty/Desktop/Coding/Specialisterne/w3-project/client/src/components/Item.js",[],"/Users/evandoherty/Desktop/Coding/Specialisterne/w3-project/client/src/models/category.js",[],"/Users/evandohert